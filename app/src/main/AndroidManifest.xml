<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dronfies.portableutmandroidclienttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PortableUTMAndroidClient"
        android:usesCleartextTraffic="true">
        <activity android:name=".EndpointsActivity"></activity>
        <activity android:name=".OperationInfoActivity" />
        <activity android:name=".OperationsActivity" />
        <activity android:name=".VehicleInfo" />
        <activity android:name=".VehiclesActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>